<% if p('influxdb.https_enabled') == 'true' %>
{
"id": "10000",
"name": "InfluxDB",
"username": "<%= p('influxdb.user') %>",
"password": "<%= p('influxdb.password') %>",
"url": "https://localhost:<%= p('influxdb.http.bind-address') %>",
"type":"influx",
"insecureSkipVerify": true,
"default": true,
"telegraf":"telegraf",
"organization":"default"
}
<% else %>
{
"id": "10000",
"name": "InfluxDB",
"username": "<%= p('influxdb.user') %>",
"password": "<%= p('influxdb.password') %>",
"url": "http://localhost:<%= p('influxdb.http.bind-address') %>",
"type":"influx",
"insecureSkipVerify": false,
"default": true,
"telegraf":"telegraf",
"organization":"default"
}
<% end %>

